### bash
[submodule "bash-it"]
	path = bash-it
	url = https://github.com/Bash-it/bash-it.git

### tmux plugin manager (all tmux plugins are downloaded with this manager, look in ~/.tmux.conf for list of plugins)
[submodule "tpm"]
    path = tmux/.tmux/plugins/tpm
    url = https://github.com/tmux-plugins/tpm.git
